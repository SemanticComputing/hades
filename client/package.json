{
  "name": "hades",
  "version": "0.0.1",
  "description": "Semantic entity search",
  "main": "dist",
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --mode development --port 3000",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "testMatch": [
      "**/src/test/**/*"
    ],
    "testPathIgnorePatterns": [
      "__snapshots__",
      "fixtures"
    ]
  },
  "author": "apoksane <arttu.oksanen@aalto.fi>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "express": "^4.16.2",
    "history": "<4.0.0",
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "moment": "^2.21.0",
    "morgan": "^1.8.0",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-autocomplete": "^1.5.6",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.4.2",
    "react-paginate": "^4.4.4",
    "react-router": "<4.0.0",
    "react-tooltip": "^3.3.0",
    "resource-router-middleware": "^0.6.0",
    "serve-favicon": "^2.4.1",
    "superagent": "^3.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "catw": "^1.0.1",
    "css-loader": "^0.28.10",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jasmine": "^2.9.3",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.0.4",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.8.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.6",
    "nodemon": "^1.9.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.11.0",
    "style-loader": "^0.20.2",
    "watch": "^1.0.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  }
}
